import turtle
import time
from turtle import Turtle
from random import randint
#the setup
window = turtle.Screen()
window.title("turtle race")
turtle.bgcolor("forest green")
turtle.color("white")
turtle.speed(0)
turtle.penup()
turtle.setpos(-140,150)
turtle.write("Turtle race", font =("times new roman", 30))
turtle.penup()
#char
turtle.setpos(-400,-180)
turtle.color("red")
turtle.begin_fill()
turtle.pendown()
turtle.forward(800)
turtle.right(90)
turtle.forward(300)
turtle.right(90)
turtle.forward(800)
turtle.right(90)
turtle.forward(300)
turtle.right(90)
turtle.end_fill()
##finish
stamp_size=20
square_size=15
finish_line=200
turtle.color("black")
turtle.shape("square")
turtle.shapesize(square_size/stamp_size)

turtle.penup()

for i in range(10):
  turtle.setpos(100, (150-(i*square_size*2)))
  turtle.stamp()
  for j in range(10):
    turtle.setpos(100+square_size, ((150-square_size)-(j*square_size*2)))
    turtle.stamp()



turtle.hideturtle()
#turtle 1
s1 = Turtle()
s1.speed(0)
s1.color("brown")
s1.shape("turtle")
s1.penup()
s1.goto(-125,75)
s1.pendown()
#turtle 2
#turtle 2
s2 = Turtle()
s2.speed(0)
s2.color("grey")
s2.shape("turtle")
s2.penup()
s2.goto(-125,50)
s2.pendown()
#turtle 3
s3 = Turtle()
s3.speed(0)
s3.color("orange")
s3.shape("turtle")
s3.penup()
s3.goto(-125,25)
s3.pendown()
#turtle 4
s4 = Turtle()
s4.speed(0)
s4.color("teal")
s4.shape("turtle")
s4.penup()
s4.goto(-125,0)
s4.pendown()
time.sleep(1)# a pause
for i in range(80):
  s1.forward(randint(1,5))
  s2.forward(randint(1,5))
  s3.forward(randint(1,5))
  s4.forward(randint(1,5))
turtle.exitonclick()
if s1.x==100:
  print("s1 win")
elif s2.x==100:
  print("s2 win") 
elif s3.x==100:
  print("s3 win")
elif s4.x==100:
  print("s4 win")    
else:
  print("error") 
