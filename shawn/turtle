import turtle
s=turtle.Turtle()

s.forward(150)
s.right(90)
s.forward(150)
s.right(90)
s.forward(150)
s.right(90)
s.forward(150)
s.up()
s.forward(50)
s.down()
s.forward(150)
s.right(90)
s.forward(150)
s.right(90)
s.forward(150)
s.right(90)
s.forward(150)



